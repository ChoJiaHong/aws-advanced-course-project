    <body>
        <div class="d-flex" id="wrapper">
            <div id="page-content-wrapper">
                <div id="nav-template"></div>

                <div id="ELByViewId">
                    <div class="d-flex justify-content-center flex-column align-items-center">
                        <h3 class="mb-4 mt-4"> AWS Elasticache </h3>
                        <!-- <button id="buyTicketId" class="mt-4 mb-4 btn btn-primary btn-lg active" style="width: 80%;">Buy Ticket</button> -->
                        <div class="row mb-3" style="width: 100%;">
                            <div class="col-2"><button class="btn btnBasic btn-lg active" style="width: 100%; height: 100%;">backend URL</button></div>
                            <div class="col-10"><input id="backendUrlId" type="text" class="form-control" style="width: 100%; height: 100%;"></div>
                        </div>
                        <div class="row mb-3" style="width: 100%;">
                            <div class="col-2"><button id="cleanCacheId" class="btn btnBasic btn-lg active" style="width: 100%; height: 100%;">Clean Cache</button></div>
                            <div class="col-2"><button id="clearListId" class="btn btnBasic btn-lg active" style="width: 100%; height: 100%;">Clear List</button></div>
                        </div>
                        <!-- <div class="row mb-3" style="width: 100%;">
                            <div class="col-2"><button class="btn btnBasic btn-lg active" style="width: 100%; height: 100%;">SQS Queue URL</button></div>
                            <div class="col-10"><input id="sqsQueueUrlId" type="text" class="form-control" style="width: 100%; height: 100%;"></div>
                        </div> -->

                        <div class="row mb-3" style="width: 100%;">
                            <div class="col-12"><button id="listEmployeeId" class="btn btnBasic btn-lg active" style="width: 100%; height: 100%;">List Employees</button></div>
                        </div>           
                        <div class="row mb-3" style="width: 100%;">
                            <div class="col-12"><button id="listEmployeeWithElasticacheId" class="btn btnBasic btn-lg active" style="width: 100%; height: 100%;">List Employees + ElastiCache</button></div>
                        </div>           
                        
                        <div class="table-wrapper-scroll-y my-custom-scrollbar">
                        <div class="row mb-3" style="width: 100%;">
                            <div class="col-12">
                            <table class="table">
                                <thead>
                                <tr>
                                    <th scope="col">Id</th>
                                    <th scope="col">Employee</th>
                                    <th scope="col">Department</th>
                                    <th scope="col">Manager</th>
                                </tr>
                                </thead>
                                <tbody id="empListId">
                                </tbody>
                            </table>  
                            </div>   
                        </div>     
                        </div>              
