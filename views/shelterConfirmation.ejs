                        <a href="/esnDir" class="mt-3 btn btn-primary d-flex justify-content-center">Return to Directory</a>

                    </div>
                    <!-- Directory Page Content -->
                </fieldset>
            </div>
            <div id="bottom-nav-template"></div>

            <div id="search-box-view"></div>

        </div>
        <!-- /#page-content-wrapper -->
        <!-- Search pop up dialogue -->
        <div class="modal fade" id="search-box" tabindex="-1" role="dialog">
            <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header text-center">
                        <h4 class="modal-title w-100 font-weight-bold">Search</h4>
                    </div>
                    <div class="modal-body mx-3">
                        <div class="md-form mb-3">
                            <input type="text" id="keyword" class="form-control validate">
                        </div>
                    </div>
                    <div class="modal-footer d-flex justify-content-center">
                        <button type="button" class="btn btn-primary">Search</button>
                        <button type="button" class="btn btn-primary" data-dismiss="modal"
                            aria-label="Close">Back</button>
                    </div>
                </div>
            </div>
        </div>
        <!-- End of search pop up dialogue -->
    </div>
